<div class="container-md">
  <br>
    <form action="/register" method="post">
      <div class="mb-3">
        <label for="username" class="form-label">ชื่อผู้ใช้</label>
        <input
          type="text"
          class="form-control" 
          id="username"
          name="username"
          value="vanikthai"
          aria-describedby=username lHelp"
        />
       
      </div>
      <div class="mb-3">
        <label for="email" class="form-label">email</label>
        <input
          type="email"
          class="form-control"
          id="email"
          name="email"
          value="vanikthai@gmail.com"
          aria-describedby="emailHelp"
        />
      
      </div>
      <div class="mb-3">
        <label for="password" class="form-label">รหัสผ่าน</label>
        <input
          type="password"
          class="form-control"
          id="password"
          name="password"
          value="aaaaaa"
        />
      </div>
      <button type="submit" class="btn btn-primary">ตกลง</button>
    </form>
    <div class="mb-3">
      <a class="position-absolute end-50" href="/login">ลงชื่อเข้าใช้งาน</a>
    </div>
  </div>
  